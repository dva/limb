{
  "name": "limb",
  "version": "1.0.0",
  "repository": "git@github.com:dva/limb.git",
  "author": "dva",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyze:app": "source-map-explorer dist/main.*",
    "analyze:vendors": "source-map-explorer dist/vendors~main.*",
    "build": "webpack --config webpack.prod.js",
    "serve": "webpack-dev-server --config webpack.dev.js --hot",
    "serve:debug": "DEBUG=* yarn serve",
    "lint:js": "standard '**/*.js'",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:ts": "tslint '**/*.tsx'",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:css": "stylelint '**/*.scss'",
    "lint:css:fix": "yarn lint:css --fix",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "lint:fix": "yarn lint:ts:fix && yarn lint:js:fix && yarn lint:css:fix",
    "type-check": "tsc",
    "type-check:watch": "yarn type-check -- --watch",
    "browserslist": "npx browserslist"
  },
  "dependencies": {
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/debug": "^0.0.31",
    "@types/lodash": "^4.14.118",
    "@types/luxon": "^1.4.1",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.6",
    "@types/webpack": "^4.4.22",
    "@types/webpack-merge": "^4.1.3",
    "debug": "^4.0.1",
    "lodash": "^4.17.10",
    "luxon": "^1.3.3",
    "polished": "^2.3.3",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-hot-loader": "^4.3.5",
    "react-router-dom": "^4.3.1",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "source-map-explorer": "^1.6.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.0",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-merge": "^4.2.1"
  }
}
