{
  "name": "limb",
  "version": "1.0.0",
  "repository": "git@github.com:dva/limb.git",
  "author": "dva",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.{js,js.map}'",
    "build:check": "yarn type-check && yarn lint && yarn test",
    "build": "webpack --config webpack.prod.js",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:css": "stylelint 'src/**/*.{ts,tsx,scss}'",
    "lint:fix": "yarn lint:ts:fix && yarn lint:js:fix && yarn lint:css:fix",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "standard '**/*.js'",
    "lint:ts:check": "tslint-config-prettier-check ./tslint.json",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:ts:old": "tslint -p ./tsconfig.json -c ./tslint.json",
    "lint:ts": "yarn eslint . --ext .tsx,.ts",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "pretty:js": "prettier-standard '**/*.js'",
    "pretty:ts": "prettier --write 'src/**/*.{tsx,ts}'",
    "serve:debug": "DEBUG=* yarn serve",
    "serve": "webpack-dev-server --config webpack.dev.js --hot",
    "test:updateSnapshot": "yarn test -u",
    "test": "jest",
    "type-check:check": "yarn type-check --listFiles",
    "type-check:trace": "yarn type-check --traceResolution",
    "type-check:watch": "yarn type-check --watch",
    "type-check": "tsc -v && tsc"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@hot-loader/react-dom": "^16.13.0",
    "debug": "^4.0.1",
    "lodash": "^4.17.15",
    "luxon": "^1.24.1",
    "polished": "^3.6.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sanitize.css": "^11.0.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/debug": "^4.1.5",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/luxon": "^1.24.0",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.12",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpackbar": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.1",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.14.1",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "2.0.5",
    "prettier-standard": "^16.3.0",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "size-plugin": "^2.0.1",
    "source-map-explorer": "^2.4.2",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^25.5.1",
    "typescript": "3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpackbar": "^4.0.0"
  }
}
