{
  "name": "limb",
  "version": "1.0.0",
  "repository": "git@github.com:dva/limb.git",
  "author": "dva",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.{js,js.map}'",
    "build:check": "yarn type-check && yarn lint && yarn test",
    "build": "webpack --config webpack.prod.js",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:fix": "yarn lint:ts:fix && yarn lint:js:fix && yarn lint:css:fix",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "standard '**/*.js'",
    "lint:ts:check": "tslint-config-prettier-check ./tslint.json",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:ts": "tslint -p ./tsconfig.json -c ./tslint.json",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "pretty:js": "prettier-standard '**/*.js'",
    "pretty:ts": "prettier --write 'src/**/*.{tsx,ts}'",
    "serve:debug": "DEBUG=* yarn serve",
    "serve": "webpack-dev-server --config webpack.dev.js --hot",
    "test:updateSnapshot": "yarn test -u",
    "test": "jest",
    "type-check:check": "yarn type-check --listFiles",
    "type-check:trace": "yarn type-check --traceResolution",
    "type-check:watch": "yarn type-check --watch",
    "type-check": "tsc -v && tsc"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@hot-loader/react-dom": "^16.13.0",
    "debug": "^4.0.1",
    "lodash": "^4.17.20",
    "luxon": "^1.25.0",
    "polished": "^3.6.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sanitize.css": "^12.0.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/debug": "^4.1.5",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/luxon": "^1.24.4",
    "@types/node": "^14.6.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "@types/webpack": "^4.41.22",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpackbar": "^4.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "enzyme-to-json": "^3.5.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.14.1",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "2.1.1",
    "prettier-standard": "^16.4.1",
    "sass-loader": "^10.0.2",
    "serve": "^11.3.2",
    "size-plugin": "^2.0.1",
    "source-map-explorer": "^2.5.0",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3",
    "webpackbar": "^4.0.0"
  }
}
