{
  "name": "limb",
  "version": "1.0.0",
  "repository": "git@github.com:dva/limb.git",
  "author": "dva",
  "license": "MIT",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'dist/*.{js,js.map}'",
    "browserslist": "npx browserslist",
    "build:check": "yarn type-check && yarn lint && yarn test",
    "build": "webpack --config webpack.prod.js",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:fix": "yarn lint:ts:fix && yarn lint:js:fix && yarn lint:css:fix",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "standard '**/*.js'",
    "lint:ts:check": "tslint-config-prettier-check ./tslint.json",
    "lint:ts:fix": "yarn lint:ts --fix",
    "lint:ts": "tslint -p ./tsconfig.json -c ./tslint.json",
    "lint": "yarn lint:ts && yarn lint:js && yarn lint:css",
    "pretty:js": "prettier-standard '**/*.js'",
    "pretty:ts": "prettier --write 'src/**/*.{tsx,ts}'",
    "serve:debug": "DEBUG=* yarn serve",
    "serve": "webpack-dev-server --config webpack.dev.js --hot",
    "test:updateSnapshot": "yarn test -u",
    "test": "jest",
    "type-check:check": "yarn type-check --listFiles",
    "type-check:trace": "yarn type-check --traceResolution",
    "type-check:watch": "yarn type-check --watch",
    "type-check": "tsc -v && tsc"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.2",
    "@hot-loader/react-dom": "^16.9.0",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.139",
    "@types/luxon": "^1.15.2",
    "@types/node": "^12.7.7",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^4.1.19",
    "@types/webpack": "^4.39.1",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpackbar": "^2.6.0",
    "debug": "^4.0.1",
    "lodash": "^4.17.15",
    "luxon": "^1.19.3",
    "polished": "^3.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.14",
    "react-router-dom": "^5.1.0",
    "sanitize.css": "^11.0.0",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/enzyme-to-json": "^1.5.3",
    "@types/jest": "^24.0.18",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "node-sass": "^4.12.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "1.18.2",
    "prettier-standard": "^14.0.7",
    "sass-loader": "^8.0.0",
    "serve": "^11.1.0",
    "size-plugin": "^2.0.0",
    "source-map-explorer": "^2.1.0",
    "standard": "^14.3.1",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2",
    "webpackbar": "^4.0.0"
  }
}
