{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react-hooks",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": [
      true,
      "log"
    ],
    "no-implicit-dependencies": [true, ["utils"]],
    "no-unused-variable": true,
    "ordered-imports": [false]
  }
}
