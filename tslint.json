{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rules": {
    "no-console": {
      "severity": "warning"
    },
    "no-unused-variable": true,
    "ordered-imports": [false],
    "quotemark": [true, "single"]
  }
}
