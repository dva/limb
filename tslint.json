{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "no-console": {
      "severity": "warning"
    },
    "ordered-imports": [false],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["**/node_modules/**"]
  }
}
